{
    "name": "brahimbim/laravel-analytics-bugfix",
    "description": "A Laravel package to retrieve Google Analytics data, with a bugfix for Laravel 8 and PHP 8",
    "keywords": [
        "spatie",
        "google",
        "analytics",
        "retrieve",
        "reports",
        "laravel"
    ],
    "homepage": "https://github.com/brahimbim/laravel-analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Brahim Bim",
            "email": "brahimbim@example.com",
            "homepage": "https://example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "google/analytics-data": "^0.9.4",
        "laravel/framework": "^8.0",
        "nesbot/carbon": "^2.66",
        "spatie/laravel-package-tools": "^1.12.1",
        "symfony/cache": "^6.0"
    },
    "require-dev": {
        "league/flysystem": "^3.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "spatie/ray": "^1.37"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Analytics\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Analytics\\AnalyticsServiceProvider"
            ],
            "aliases": {
                "Analytics": "Spatie\\Analytics\\Facades\\Analytics"
            }
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Analytics\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
